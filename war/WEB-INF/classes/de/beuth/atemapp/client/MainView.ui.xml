<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client" xmlns:gwt="urn:import:com.google.gwt.user.client.ui"
	xmlns:button="urn:import:com.sencha.gxt.widget.core.client.button"
	xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container">

	<ui:with field="themeStyles"
		type="com.sencha.gxt.core.client.resources.ThemeStyles.Styles" />

	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="outerMargins">
		<ui:attributes top="4" right="4" bottom="4" left="4" />
	</ui:with>
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="centerMargins">
		<ui:attributes top="0" right="4" bottom="0" left="4" />
	</ui:with>

	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="topLeftMargins">
		<ui:attributes top="4" right="0" bottom="0" left="4" />
	</ui:with>

	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="topLeftRightMargins">
		<ui:attributes top="4" right="0" bottom="0" left="4" />
	</ui:with>

	<ui:with
		type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData"
		field="topData">
		<ui:attributes width="1" height=".5d" margins="{outerMargins}" />
	</ui:with>
	<ui:with
		type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData"
		field="middleData">
		<ui:attributes width="1" height="-200" margins="{centerMargins}" />
	</ui:with>

	<ui:with
		type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData"
		field="bottomData">
		<ui:attributes width="1" height=".5d" margins="{outerMargins}" />
	</ui:with>

	<ui:with
		type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData"
		field="buttonData">
		<ui:attributes width="16" height="16" margins="{topLeftMargins}" />
	</ui:with>

	<ui:with
		type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData"
		field="buttonRightData">
		<ui:attributes width="16" height="16" margins="{topLeftRightMargins}" />
	</ui:with>

	<container:HorizontalLayoutContainer
		pixelSize="1500, 600">
		<container:child>
			<gxt:FramedPanel collapsible="false" headingText="Mix Effects">
				<container:VerticalLayoutContainer>
					<container:child layoutData="{topData}">
						<gxt:FramedPanel headingText="Program">
							<gwt:HorizontalPanel>
								<gwt:CaptionPanel captionText="Input">
									<gwt:VerticalPanel spacing="10">
										<gwt:HorizontalPanel spacing="5">
											<button:ToggleButton ui:field="prog_input_black"
												text="Black" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prog_input_1"
												text="1" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prog_input_2"
												text="2" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prog_input_3"
												text="3" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prog_input_4"
												text="4" pixelSize="40, 40" />
										</gwt:HorizontalPanel>
										<gwt:HorizontalPanel spacing="5">
											<button:ToggleButton ui:field="prog_input_bars"
												text="Bars" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prog_input_5"
												text="5" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prog_input_6"
												text="6" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prog_input_7"
												text="7" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prog_input_8"
												text="8" pixelSize="40, 40" />
										</gwt:HorizontalPanel>
									</gwt:VerticalPanel>
								</gwt:CaptionPanel>
								<gwt:VerticalPanel>
									<gwt:CaptionPanel captionText="Color">
										<gwt:HorizontalPanel spacing="5">
											<button:ToggleButton ui:field="prog_color_1"
												text="1" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prog_color_2"
												text="2" pixelSize="40, 40" />
										</gwt:HorizontalPanel>
									</gwt:CaptionPanel>
									<gwt:CaptionPanel captionText="Media">
										<gwt:HorizontalPanel spacing="5">
											<button:ToggleButton ui:field="prog_media_1"
												text="1" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prog_media_2"
												text="2" pixelSize="40, 40" />
										</gwt:HorizontalPanel>
									</gwt:CaptionPanel>
								</gwt:VerticalPanel>
							</gwt:HorizontalPanel>
						</gxt:FramedPanel>
					</container:child>
					<container:child layoutData="{bottomData}">
						<gxt:FramedPanel headingText="Preview"
							collapsible="true">
							<gwt:HorizontalPanel>
								<gwt:CaptionPanel captionText="Input">
									<gwt:VerticalPanel spacing="10">
										<gwt:HorizontalPanel spacing="5">
											<button:ToggleButton ui:field="prev_input_black"
												text="Black" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prev_input_1"
												text="1" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prev_input_2"
												text="2" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prev_input_3"
												text="3" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prev_input_4"
												text="4" pixelSize="40, 40" />
										</gwt:HorizontalPanel>
										<gwt:HorizontalPanel spacing="5">
											<button:ToggleButton ui:field="prev_input_bars"
												text="Bars" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prev_input_5"
												text="5" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prev_input_6"
												text="6" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prev_input_7"
												text="7" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prev_input_8"
												text="8" pixelSize="40, 40" />
										</gwt:HorizontalPanel>
									</gwt:VerticalPanel>
								</gwt:CaptionPanel>
								<gwt:VerticalPanel>
									<gwt:CaptionPanel captionText="Color">
										<gwt:HorizontalPanel spacing="5">
											<button:ToggleButton ui:field="prev_color_1"
												text="1" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prev_color_2"
												text="2" pixelSize="40, 40" />
										</gwt:HorizontalPanel>
									</gwt:CaptionPanel>
									<gwt:CaptionPanel captionText="Media">
										<gwt:HorizontalPanel spacing="5">
											<button:ToggleButton ui:field="prev_media_1"
												text="1" pixelSize="40, 40" />
											<button:ToggleButton ui:field="prev_media_2"
												text="2" pixelSize="40, 40" />
										</gwt:HorizontalPanel>
									</gwt:CaptionPanel>
								</gwt:VerticalPanel>
							</gwt:HorizontalPanel>
						</gxt:FramedPanel>
					</container:child>
				</container:VerticalLayoutContainer>
			</gxt:FramedPanel>
		</container:child>
		<container:child>
			<gxt:FramedPanel headingText="Transition Control">
				<gwt:HorizontalPanel spacing="5">
					<gwt:VerticalPanel>
						<gwt:CaptionPanel captionText="Next Transition">
							<gwt:VerticalPanel spacing="5">
								<gwt:HorizontalPanel spacing="5">
									<gwt:HTMLPanel pixelSize="40, 40" />
									<button:ToggleButton ui:field="trans_ctrl_next_onair_1"
										text="On Air" pixelSize="40, 40" />
									<button:ToggleButton ui:field="trans_ctrl_next_onair_2"
										text="On Air" pixelSize="40, 40" />
									<button:ToggleButton ui:field="trans_ctrl_next_onair_3"
										text="On Air" pixelSize="40, 40" />
									<button:ToggleButton ui:field="trans_ctrl_next_onair_4"
										text="On Air" pixelSize="40, 40" />
								</gwt:HorizontalPanel>
								<gwt:HorizontalPanel spacing="5">
									<button:ToggleButton ui:field="trans_ctrl_next_bkgd"
										text="BKGD" pixelSize="40, 40" />
									<button:ToggleButton ui:field="trans_ctrl_next_key_1"
										text="Key 1" pixelSize="40, 40" />
									<button:ToggleButton ui:field="trans_ctrl_next_key_2"
										text="Key 2" pixelSize="40, 40" />
									<button:ToggleButton ui:field="trans_ctrl_next_key_3"
										text="Key 3" pixelSize="40, 40" />
									<button:ToggleButton ui:field="trans_ctrl_next_key_4"
										text="Key 4" pixelSize="40, 40" />
								</gwt:HorizontalPanel>
							</gwt:VerticalPanel>
						</gwt:CaptionPanel>
						<gwt:CaptionPanel captionText="Transition Style">
							<gwt:VerticalPanel>
								<gwt:HorizontalPanel spacing="5">
									<button:ToggleButton ui:field="trans_ctrl_style_mix"
										text="MIX" pixelSize="40, 40" />
									<button:ToggleButton ui:field="trans_ctrl_style_dip"
										text="DIP" pixelSize="40, 40" />
									<button:ToggleButton ui:field="trans_ctrl_style_wipe"
										text="WIPE" pixelSize="40, 40" />
									<button:ToggleButton ui:field="trans_ctrl_style_dve"
										text="DVE" pixelSize="40, 40" />
									<button:ToggleButton ui:field="trans_ctrl_style_sting"
										text="STING" pixelSize="40, 40" />
								</gwt:HorizontalPanel>
								<gwt:HorizontalPanel spacing="5">
									<button:ToggleButton ui:field="trans_ctrl_style_prev_trans"
										text="PREV" pixelSize="40, 40" styleName="toggleButton" />
									<gwt:VerticalPanel spacing="5">
										<gwt:Label text="RATE" pixelSize="70, 13" />
										<gwt:TextBox text="0" pixelSize="70, 13" />
									</gwt:VerticalPanel>
									<button:ToggleButton ui:field="trans_ctrl_style_auto"
										text="AUTO" pixelSize="40, 40" />
									<button:ToggleButton ui:field="trans_ctrl_style_cut"
										text="CUT" pixelSize="40, 40" />
								</gwt:HorizontalPanel>
							</gwt:VerticalPanel>
						</gwt:CaptionPanel>
					</gwt:VerticalPanel>
					<gxt:Slider ui:field="transition_slider" increment="1"
						height="300" />
				</gwt:HorizontalPanel>
			</gxt:FramedPanel>
		</container:child>
		<container:child>
			<gwt:VerticalPanel>
				<gxt:FramedPanel headingText="Downstream Key">
					<gwt:HorizontalPanel spacing="5">
						<gwt:VerticalPanel spacing="5">
							<gwt:Label text="DSK 1" />
							<button:ToggleButton ui:field="downstream_dsk_1_tie"
								text="TIE" pixelSize="40, 40" />
							<gwt:Label text="RATE" />
							<gwt:TextBox ui:field="downstream_dsk_1_rate" text="0"
								pixelSize="40, 20" />
							<button:ToggleButton ui:field="downstream_dsk_1_onair"
								text="On Air" pixelSize="40, 40" />
							<button:ToggleButton ui:field="downstream_dsk_1_auto"
								text="AUTO" pixelSize="40, 40" />
						</gwt:VerticalPanel>
						<gwt:VerticalPanel spacing="5">
							<gwt:Label text="DSK 2" />
							<button:ToggleButton ui:field="downstream_dsk_2_tie"
								text="TIE" pixelSize="40, 40" />
							<gwt:Label text="RATE" />
							<gwt:TextBox ui:field="downstream_dsk_2_rate" text="0"
								pixelSize="40, 20" />
							<button:ToggleButton ui:field="downstream_dsk_2_onair"
								text="On Air" pixelSize="40, 40" />
							<button:ToggleButton ui:field="downstream_dsk_2_auto"
								text="AUTO" pixelSize="40, 40" />
						</gwt:VerticalPanel>
					</gwt:HorizontalPanel>
				</gxt:FramedPanel>
				<gxt:FramedPanel headingText="Fade to Black">
					<gwt:HorizontalPanel spacing="5">
						<gwt:VerticalPanel>
							<gwt:Label text="RATE" />
							<gwt:TextBox ui:field="ftb_rate" text="0"
								pixelSize="40, 20" />
						</gwt:VerticalPanel>
						<button:ToggleButton ui:field="ftb" text="AUTO"
							pixelSize="40, 40" />
					</gwt:HorizontalPanel>
				</gxt:FramedPanel>
			</gwt:VerticalPanel>
		</container:child>
	</container:HorizontalLayoutContainer>
</ui:UiBinder> 