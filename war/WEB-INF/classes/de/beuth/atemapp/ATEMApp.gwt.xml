<?xml version="1.0" encoding="UTF-8"?>
<!-- When updating your version of GWT, you should also update this DTD reference, 
	so that your app can take advantage of the latest GWT module capabilities. -->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN"
  "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='atemapp'>

	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.core.Core' />
	<inherits name="org.atmosphere.gwt.Client" />
	<inherits name="com.google.gwt.logging.Logging" />
	<inherits name="com.sencha.gxt.ui.GXT" />
	
	<!-- <set-property name="user.agent" value="safari" /> -->

	<set-property name="gwt.logging.logLevel" value="FINEST" />
	<set-property name="gwt.logging.popupHandler" value="DISABLED" />

	<!-- inherit css based theme -->
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard' /> -->

	<set-configuration-property name="UiBinder.useSafeHtmlTemplates"
		value="true" />
	<!-- <set-configuration-property name="mgwt.css"
		value="pretty" /> -->

	<!-- <set-property name="user.agent" value="safari" /> -->
	<set-property name="locale" value="default" />

	<!-- Specify the app entry point class. -->
	<entry-point class='de.beuth.atemapp.client.ATEMApp' />

	<source path="client" />
	<source path="shared" />

</module>
